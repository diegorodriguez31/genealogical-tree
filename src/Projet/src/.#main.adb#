with Ada.Text_IO; use Ada.Text_IO;
with Ada.Integer_Text_IO; use Ada.Integer_Text_IO;
with P_Menu; use P_Menu;
with P_Arbre_Genealog; use P_Arbre_Genealog;

procedure Main is

   -- Instanciation de l'arbre racine principal qui va permettre de générer l'arbre généalogique
   arbre_racine : arbre_genealogique.T_Arbre_Bin;

begin
   Put_Line("Bonjour, vous etes sur le point de creer un arbre genealogique.");
   -- Cette fonction fait appel au menu qui va mettre en place tout le programme
   manipulation_arbre_gen(arbre_racine);
end Main;
